    // },
    // {
    //   "after": ["w", "w", "w", "w", "w"],
    //   "before": ["right"]
    // },
    {
      "before": ["<leader>", "q"],
      "commands": ["workbench.action.closeEditorsInGroup"]
    },
    {
      "before": ["<leader>", "h"],
      "commands": ["workbench.action.navigateLeft"]
    },
    {
      "before": ["<leader>", "j"],
      "commands": ["workbench.action.navigateDown"]
    },
    {
      "before": ["<leader>", "k"],
      "commands": ["workbench.action.navigateUp"]
    },
    {
      "before": ["<leader>", "l"],
      "commands": ["workbench.action.navigateRight"]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.view.explorer"]
    },
    {
      "after": ["\"", "_", "d"],
      "before": ["<leader>", "d"]
    },
    {
      "after": ["\"", "_", "D"],
      "before": ["<leader>", "D"]
    }
  ],
  // "vim.easymotion": true,
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.commandlineinprogress": ["#4ec9b0", "#000000"],
  "vim.statusBarColors.easymotioninputmode": "#007ACC",
  "vim.statusBarColors.easymotionmode": "#00bcd4",
  "vim.statusBarColors.insert": ["#1e1e1e", "#cccccc"],
  "vim.statusBarColors.normal": ["#424242", "#cccccc"],
  "vim.statusBarColors.replace": "#d04573",
  "vim.statusBarColors.searchinprogressmode": ["#e69c2e", "#000000"],
  "vim.statusBarColors.surroundinputmode": "#007ACC",
  "vim.statusBarColors.visual": ["#c586c0", "#000000"],
  "vim.statusBarColors.visualblock": ["#c586c0", "#000000"],
  "vim.statusBarColors.visualline": ["#c586c0", "#000000"],
  "vim.useCtrlKeys": true,
  "vim.useSystemClipboard": true,
