  "git.autofetch": true,
  "git.confirmSync": false,
  "git.suggestSmartCommit": false,
  "python.languageServer": "Pylance",
  "python.showStartPage": false,
  "terminal.integrated.fontFamily": "MesloLGS NF",
  // vim.incsearch": true,
  "vim.leader": "<space>",
  "vim.normalModeKeyBindings": [
    // {
    //   "after": ["k", "k", "k", "k", "k"],
    //   "before": ["up"]
    // },
    // {
    //   "after": ["j", "j", "j", "j", "j"],
